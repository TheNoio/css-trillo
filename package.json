{
  "name": "Trillo",
  "version": "1.0.0",
  "description": "Trillo APP",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "serve": "concurrently \"npm run watch:sass\" \"npx live-server --ignore=node_modules .\"",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheNoio/css-trillo.git"
  },
  "keywords": [],
  "author": "Naio",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TheNoio/css-trillo/issues"
  },
  "homepage": "https://github.com/TheNoio/css-trillo#readme",
  "dependencies": {
    "css-trillo": "file:",
    "Trillo": "file:"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "live-server": "^1.2.2",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.0"
  }
}
